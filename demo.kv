                on_release:
                    #app.change_screen('trophy','left')
                    app.trophys()
                AnchorLayout:
                    pos_hint: {"center_x": .5, "center_y": .5}
                    canvas.before:
                        PushMatrix
                        Rotate:
                            angle: app.menurotate
                            origin: self.center
                    canvas.after:
                        PopMatrix

                    FitImage:
                        source: "images/trophy.png"
                        size_hint: app.menuscale
            MyTile:
                text: "[size=26]Info[/color]"
